    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>5d10cde2-202a-494c-a6db-03da65a0dabe</UserSecretsId>
    <DockerDefaultTargetOS>Linux</DockerDefaultTargetOS>
